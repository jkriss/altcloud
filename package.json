{
  "name": "altcloud",
  "version": "0.0.7",
  "description": "A web server with extra powers. Run your own stuff.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jkriss/altcloud.git"
  },
  "license": "MIT",
  "bin": {
    "altcloud": "bin/altcloud.js"
  },
  "scripts": {
    "test": "tape tests/**/**/*.js | faucet",
    "lint": "prettier -write \"{bin,lib,tests}/**/*.js\"",
    "precommit": "lint-staged",
    "nexe":
      "nexe bin/altcloud.js -o binaries/macos/altcloud -r \"node_modules/localhost.daplie.com-certificates/*.pem\" -r \"node_modules/blake2b-wasm/blake2b.wasm\"",
    "pkg-mac":
      "pkg --targets node8-macos-x64 --output binaries/macos/altcloud package.json",
    "pkg-linux":
      "pkg --targets node8-linux-x64 --output binaries/linux-x64/altcloud package.json",
    "pkg-win":
      "pkg --targets node8-win-x64 --output binaries/win/altcloud.exe package.json",
    "pkg-arm6":
      "pkg --targets node8-linux-armv6 --output binaries/linux-armv6/altcloud package.json",
    "pkg-arm7":
      "pkg --targets node8-linux-armv7 --output binaries/linux-armv7/altcloud package.json",
    "pkg":
      "npm run pkg-arm6 ; npm run pkg-arm7 ; npm run pkg-mac ; npm run pkg-linux ; npm run pkg-win",
    "upload":
      "rsync -rva binaries/* altcloud@tinyserver.club:~/webroot/altcloud.io/"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "author": "Jesse Kriss <jesse@jklabs.net>",
  "dependencies": {
    "array-unique": "^0.3.2",
    "basic-auth": "^1.1.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "common-password": "^0.1.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "cron": "^1.2.1",
    "cron-parser": "^2.3.0",
    "debug": "^3.1.0",
    "express": "^4.14.0",
    "front-matter": "^2.1.1",
    "greenlock-express": "^2.0.12",
    "handlebars": "^4.0.11",
    "helmet": "^3.4.0",
    "http-errors": "^1.5.1",
    "ip": "^1.1.4",
    "js-yaml": "^3.7.0",
    "jsonwebtoken": "^7.2.1",
    "liquid-node": "^2.6.1",
    "marked": "^0.3.6",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pem": "^1.9.4",
    "polo": "^0.8.1",
    "route-parser": "0.0.5",
    "slug": "^0.9.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "del": "^2.2.2",
    "faucet": "0.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "nexe": "^2.0.0-rc.22",
    "node-mocks-http": "^1.5.5",
    "pkg": "4.2.2",
    "prettier": "1.9.2",
    "supertest": "^2.0.1",
    "tape": "^4.6.3"
  },
  "pkg": {
    "assets": [
      "node_modules/localhost.daplie.com-certificates/*.pem",
      "node_modules/blake2b-wasm/blake2b.wasm"
    ]
  }
}
