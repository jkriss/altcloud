{
  "name": "altcloud",
  "version": "0.0.7",
  "description": "A web server with extra powers. Run your own stuff.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jkriss/altcloud.git"
  },
  "license": "MIT",
  "bin": {
    "altcloud": "bin/altcloud.js"
  },
  "scripts": {
    "test": "standard && tape tests/**/**/*.js | faucet",
